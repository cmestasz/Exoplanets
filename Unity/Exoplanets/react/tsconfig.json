  {
    "extends": "@reactunity/scripts",
    "compilerOptions": {
      "baseUrl": "./",
      "jsxImportSource": "@reactunity/renderer/ugui",
      "types": [
        "@reactunity/scripts/main"
      ],
      "paths": {
        "@components/*": [
          "src/components/*"
        ],
        "@lib/*": [
          "src/lib/*"
        ],
        "@styles/*": [
          "src/styles/*"
        ],
        "@types/*": [
          "src/types/*"
        ]
      }
    },
    "files": [
      "src/index.tsx",
    ],
    "include": [
      "./src",
      "tailwind.config.js",
      "webpack.config.js"
    ],
  }